{
    "containerDefinitions": [
      {
        "name": "web",
        "image": "REPOSITORY_URI:IMAGE_TAG",
        "portMappings": [
          {
            "containerPort": 8080,
            "hostPort": 8080,
            "protocol": "tcp"
          }
        ],
        "essential": true
      }
    ],
    "taskRoleArn": "TASK_ROLE_ARN",
    "executionRoleArn": "EXECUTION_ROLE_ARN",
    "cpu": "256",
    "memory": "512"
  }